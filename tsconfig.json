{
  "compilerOptions": {
    "jsx": "react",
    "target": "esnext",
    "module": "commonjs",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noEmit": true,
    "baseUrl": "./src",
    "paths": {
      "res": ["res"],
      "contexts": ["contexts"],
      "views": ["views"],
      "components": ["components"],
      "helpers": ["helpers"]
    }
  }
}
